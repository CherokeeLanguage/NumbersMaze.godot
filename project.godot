[application]

config/name="Numbers Maze"
config/description="Cherokee Numbers Maze"
run/main_scene="res://main/Main.tscn"
boot_splash/image="res://graphics/splash/logo.png"
boot_splash/use_filter=false
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://icon.png"
config/auto_accept_quit=false
config/quit_on_go_back=false

[compression]

formats/zstd/long_distance_matching=true
formats/zstd/compression_level=22
formats/zlib/compression_level=9
formats/gzip/compression_level=9

[display]

window/size/width=1920
window/size/height=1080
window/size/test_width=1280
window/size/test_height=720
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[gui]

theme/use_hidpi=true

